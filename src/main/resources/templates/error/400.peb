{% import "widget/back-button" %}

{% extends "layout" %}

{% block title %}400{% endblock %}

{% block content %}
    <div class="panel">
        <div class="panel-heading">
            <p>400 Error</p>
        </div>
        <div class="panel-body">
            <p>
                <strong>Something went wrong with your request.</strong> <br>
                Please check the URL or click the button below to return to the homepage.
            </p>
        </div>
    </div>
    {{ backButton('/') }}
{% endblock %}